{
  "name": "@bwi/server",
  "version": "1.6.3",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "demon": "nodemon index.js",
    "migrane": "node ./db/migrations.js",
    "build": "webpack --config ./webpack.config.js",
    "lint": "esw *.js server config --color",
    "lint:watch": "yarn lint -- --watch",
    "precommit": "yarn lint && yarn test",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors server --recursive",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors server --recursive",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "dependencies": {
    "@bwi/shared": "^1.6.3",
    "@keyv/sqlite": "^2.0.2",
    "babel-eslint": "^10.1.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-history-api-fallback": "^1.6.0",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^3.4.0",
    "excel4node": "^1.7.2",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.1.0",
    "express-rate-limit": "^5.3.0",
    "express-session": "^1.17.2",
    "helmet": "^4.6.0",
    "http-status": "^1.5.0",
    "joi": "^17.4.1",
    "jsonwebtoken": "^8.5.1",
    "keyv": "^4.0.3",
    "knex": "^0.95.7",
    "lodash": "^4.17.21",
    "mocha": "^9.1.1",
    "mongoose": "^6.0.4",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "prettier": "^2.3.2",
    "sqlite3": "^5.0.2",
    "uuid": "^8.3.2",
    "validator": "^13.6.0",
    "winston": "^3.3.3",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "istanbul": "^0.4.5",
    "nodemon": "^2.0.11",
    "webpack": "^5.47.1",
    "webpack-cli": "^4.7.2"
  }
}
